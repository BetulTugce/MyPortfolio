@model List<MyPortfolio.DAL.Entities.Skill>
@{
    string percentValue;
}
<div class="col-six tab-full right">
    <h3>I've Got Some skills.</h3>

    <ul class="skill-bars">
        @foreach (var skill in Model)
        {
            percentValue = "progress percent" + skill.Value.ToString();
            <li>
                <div class="@percentValue"><span>@skill.Value%</span></div>
                <strong>@skill.Title</strong>
            </li>
        }
    </ul>
</div>
